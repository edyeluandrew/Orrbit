# Orbit Platform Configuration Template
# Copy this file to .env and fill in your values
# NEVER commit .env to git!

# Platform wallet address (receives the 2% fee)
VITE_PLATFORM_WALLET=YOUR_PLATFORM_WALLET_PUBLIC_KEY

# Admin wallet addresses (comma-separated, no spaces)
# These wallets can access Providers and Dashboard tabs
VITE_ADMIN_WALLETS=ADMIN_WALLET_1,ADMIN_WALLET_2

# Platform fee percentage (e.g., 2 = 2%)
VITE_PLATFORM_FEE_PERCENT=2

# Network: testnet or mainnet
VITE_NETWORK=testnet

# WalletConnect Project ID (get yours at https://cloud.walletconnect.com)
# Required for LOBSTR wallet integration
VITE_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Backend API URL
VITE_API_URL=http://localhost:3001/api

# WebSocket URL
VITE_WS_URL=ws://localhost:3001
